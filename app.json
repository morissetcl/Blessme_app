{
  "expo": {
    "name": "Bless Me",
    "slug": "Blessme_app",
    "privacy": "public",
    "sdkVersion": "36.0.0",
    "platforms": [
      "ios",
      "android"
    ],
    "version": "0.1.0",
    "orientation": "portrait",
    "icon": "./assets/blessmelogo.png",
    "splash": {
      "image": "./assets/blessmelogo.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "android": {
      "package": "com.blessme.blessme",
      "versionCode": 3,
      "permissions": [
        "CAMERA",
        "RECORD_AUDIO",
        "READ_PHONE_STATE",
        "READ_CONTACTS"
      ]
    },
    "orientation": "portrait",
    "icon": "./assets/blessmelogo.png",
    "notification": {
      "icon": "./assets/blessmelogo.png"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "description": "Bless Me is an christian application dedicated to intercessions. You can post your prayer request or pray for those who need help. You can write a prayer or record one."
  }
}
