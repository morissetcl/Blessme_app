{
  "expo": {
    "name": "Bless Me",
    "slug": "Blessme_app",
    "privacy": "public",
    "sdkVersion": "36.0.0",
    "platforms": [
      "ios",
      "android"
    ],
    "version": "0.1.1",
    "orientation": "portrait",
    "icon": "./assets/splash.png",
    "android": {
      "splash": {
        "icon": "./assets/splash.png",
        "ldpi": "./assets/splash.png",
        "mdpi": "./assets/splash.png",
        "hdpi": "./assets/splash.png",
        "xhdpi": "./assets/splash.png",
        "xxhdpi": "./assets/splash.png",
        "xxxhdpi": "./assets/splash.png",
        "resizeMode": "contain",
        "backgroundColor": "#ffffff"
      },
      "package": "com.blessme.blessme",
      "versionCode": 4,
      "permissions": [
        "CAMERA",
        "RECORD_AUDIO",
        "READ_PHONE_STATE",
        "READ_CONTACTS"
      ]
    },
    "orientation": "portrait",
    "icon": "./assets/splash.png",
    "notification": {
      "icon": "./assets/splash.png",
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "splash": {
        "icon": "./assets/splash.png",
        "ldpi": "./assets/splash.png",
        "mdpi": "./assets/splash.png",
        "hdpi": "./assets/splash.png",
        "xhdpi": "./assets/splash.png",
        "xxhdpi": "./assets/splash.png",
        "xxxhdpi": "./assets/splash.png",
        "resizeMode": "contain",
        "backgroundColor": "#ffffff"
      }
    },
    "description": "Bless Me is an christian application dedicated to intercessions. You can post your prayer request or pray for those who need help. You can write a prayer or record one."
  }
}
